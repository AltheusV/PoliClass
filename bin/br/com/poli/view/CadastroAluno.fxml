            <Image url="br/com/poli/resources/logoPOLI.png" />
         </image>
      </ImageView>
      <Label layoutX="410.0" layoutY="46.0" prefHeight="105.0" prefWidth="311.0" text="Preencha as informações:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="360.0" layoutY="127.0" text="Nome do aluno:" />
